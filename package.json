{
  "version": "1.0.0",
  "description": "Stressless styles for components using js. Write CSS with functional mixins according to the component structure using components names",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "cross-env NODE_ENV=production babel -d ./build/rockey ./pakcages/rockey/",
    "test-dev": "jest --watch",
    "format": "prettier --trailing-comma es5 --single-quote --write 'packages/*/*.js' 'packages/*/!(node_modules)/**/*.js'",
    "optimize-parse": "node ./packages/rockey/tasks/optimizeParse.js",
    "best-results": "cross-env NODE_ENV=development babel-node ./packages/rockey/tasks/bestResults.js",
    "dev": "node ./tasks/dev",
    "clean": "node ./tasks/clean",
    "minify": "node ./tasks/minify",
    "precommit": "lint-staged"
  },
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "rockey"
  ],
  "author": "Valerii Sorokobatko <valeriy.sorokobatko@gmail.com> (https://github.com/tuchk4)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tuchk4/rockey/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuchk4/rockey.git"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^2.2.0",
    "chalk": "^1.1.3",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.1.0",
    "eslint": "^3.18.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gzip-size": "^3.0.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "lerna": "2.0.0-beta.37",
    "lint-staged": "^3.4.0",
    "postcss": "^5.2.16",
    "postcss-nested": "^1.0.0",
    "postcss-safe-parser": "^2.0.0",
    "prettier": "^1.2.2",
    "pretty-bytes": "^4.0.2",
    "react-test-renderer": "^15.4.2",
    "recompose": "^0.23.1",
    "rimraf": "^2.6.1",
    "spawn-sync": "^1.0.15",
    "webpack": "^2.2.1",
    "yargs": "^7.0.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^15.5.4"
  }
}
